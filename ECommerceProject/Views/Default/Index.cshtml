@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="zxx">
@Html.Action("PartialHead", "Default")
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Humberger Begin -->
    <!-- Humberger End -->
    <!-- Header Section Begin -->
    @Html.Action("PartialNavbar", "Default")
    <!-- Header Section End -->
    <!-- Hero Section Begin -->
    <section class="hero">
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <div class="hero__categories">
                        <div class="hero__categories__all">
                            <i class="fa fa-bars"></i>
                            <span>Tüm Kategoriler</span>
                        </div>
                        <ul>
                            <li><a href="/BestSeller/Index/">Çok Satanlar</a></li>
                            <li><a href="/NewSeason/Index/">Yeni Sezon</a></li>
                            <li><a href="/WomanClothes/Index/">Kadın</a></li>
                            <li><a href="#">Erkek</a></li>
                            <li><a href="/ChildrenClothes/Index/">Çocuk</a></li>
                            <li><a href="#">Ayakkabı</a></li>
                            <li><a href="/Bags/Index/">Çanta</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-9">
                    <div class="hero__search">
                        <div class="hero__search__form">
                            <form action="#">
                                <div class="hero__search__categories">
                                    Tüm Kategoriler
                                    <span class="arrow_carrot-down"></span>
                                </div>
                                <input type="text" placeholder="Ne aramıştınız?">
                                <button type="submit" class="site-btn">ARA</button>
                            </form>
                        </div>
                        <div class="hero__search__phone">
                            <div class="hero__search__phone__icon">
                                <i class="fa fa-phone"></i>
                            </div>
                            <div class="hero__search__phone__text">
                                <h5>+90 555 111 22 33</h5>
                                <span>7/24 destek hattı</span>
                            </div>
                        </div>
                    </div>
                    <div id="home" class="hero__item set-bg" data-setbg="https://bogatyr.club/uploads/posts/2023-02/3351/1675513205_bogatyr-club-p-serii-gradient-foni-4.jpg">
                        <div class="hero__text">
                            <span>YENİ SEZON</span>
                            <h2>VELURIUM <br /></h2>
                            <p>Ücretsiz teslimat ve mağazadan teslim alma</p>
                            <a href="#" class="primary-btn">ALIŞVERİŞE BAŞLA</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Hero Section End -->
    <!-- Categories Section Begin -->
    @Html.Action("PartialCategory", "Default")
    <br />
    <!-- Categories Section End -->
    <!-- Featured Section Begin -->
    @*@Html.Action("PartialFeature", "Default")
    <br />*@
    <!-- Featured Section End -->
    <!-- Banner Begin -->
    @Html.Action("PartialBanner", "Default")
    <!-- Banner End -->
    <!-- Latest Product Section Begin -->
    @Html.Action("PartialProduct", "Default")
    <!-- Latest Product Section End -->
    <!-- Blog Section Begin -->
    @Html.Action("PartialBlog", "Default")
    <!-- Blog Section End -->
    <!-- Footer Section Begin -->
    @Html.Action("PartialFooter", "Default")
    <!-- Footer Section End -->
    <!-- Js Plugins -->
    <script src="~/ogani-master/ogani-master/js/jquery-3.3.1.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/bootstrap.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/jquery.nice-select.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/jquery-ui.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/jquery.slicknav.js"></script>
    <script src="~/ogani-master/ogani-master/js/mixitup.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/owl.carousel.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/main.js"></script>



</body>
</html>
