@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="zxx">
@Html.Action("PartialHead", "Default")
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Humberger Begin -->
    <div class="humberger__menu__overlay"></div>
    <div class="humberger__menu__wrapper">
        <div class="humberger__menu__logo">
            <a href="#"><img src="~/ogani-master/ogani-master/img/logo.png" alt=""></a>
        </div>
        <div class="humberger__menu__cart">
            <ul>
                <li><a href="#"><i class="fa fa-heart"></i> <span>1</span></a></li>
                <li><a href="#"><i class="fa fa-shopping-bag"></i> <span>3</span></a></li>
            </ul>
            <div class="header__cart__price">item: <span>$150.00</span></div>
        </div>
        <div class="humberger__menu__widget">
            <div class="header__top__right__language">
                <img src="~/ogani-master/ogani-master/img/language.png" alt="">
                <div>English</div>
                <span class="arrow_carrot-down"></span>
                <ul>
                    <li><a href="#">Spanis</a></li>
                    <li><a href="#">English</a></li>
                </ul>
            </div>
            <div class="header__top__right__auth">
                <a href="#"><i class="fa fa-user"></i> Login</a>
            </div>
        </div>
        <nav class="humberger__menu__nav mobile-menu">
            <ul>
                <li class="active"><a href="~/ogani-master/ogani-master/index.html">Ana Sayfa</a></li>
                <li><a href="~/ogani-master/ogani-master/shop-grid.html">Shop</a></li>
                <li>
                    <a href="#">Pages</a>
                    <ul class="header__menu__dropdown">
                        <li><a href="~/ogani-master/ogani-master/shop-details.html">Shop Details</a></li>
                        <li><a href="~/ogani-master/ogani-master/shoping-cart.html">Shoping Cart</a></li>
                        <li><a href="~/ogani-master/ogani-master/checkout.html">Check Out</a></li>
                        <li><a href="~/ogani-master/ogani-master/blog-details.html">Blog Details</a></li>
                    </ul>
                </li>
                <li><a href="~/ogani-master/ogani-master/blog.html">Blog</a></li>
                <li><a href="~/ogani-master/ogani-master/contact.html">Contact</a></li>
            </ul>
        </nav>
        <div id="mobile-menu-wrap"></div>
        <div class="header__top__right__social">
            <a href="#"><i class="fa fa-facebook"></i></a>
            <a href="#"><i class="fa fa-twitter"></i></a>
            <a href="#"><i class="fa fa-linkedin"></i></a>
            <a href="#"><i class="fa fa-pinterest-p"></i></a>
        </div>
        <div class="humberger__menu__contact">
            <ul>
                <li><i class="fa fa-envelope"></i> hello@colorlib.com</li>
                <li>Free Shipping for all Order of $99</li>
            </ul>
        </div>
    </div>
    <!-- Humberger End -->
    <!-- Header Section Begin -->
    @Html.Action("PartialNavbar", "Default")
    <!-- Header Section End -->
    <!-- Hero Section Begin -->
    <section class="hero">
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <div class="hero__categories">
                        <div class="hero__categories__all">
                            <i class="fa fa-bars"></i>
                            <span>Tüm Kategoriler</span>
                        </div>
                        <ul>
                            <li><a href="#">Çok Satanlar</a></li>
                            <li><a href="#">Yeni Gelenler</a></li>
                            <li><a href="#">Kadın</a></li>
                            <li><a href="#">Erkek</a></li>
                            <li><a href="#">Ayakkabı</a></li>
                            <li><a href="#">Çanta</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-9">
                    <div class="hero__search">
                        <div class="hero__search__form">
                            <form action="#">
                                <div class="hero__search__categories">
                                    Tüm Kategoriler
                                    <span class="arrow_carrot-down"></span>
                                </div>
                                <input type="text" placeholder="Ne aramıştınız?">
                                <button type="submit" class="site-btn">ARA</button>
                            </form>
                        </div>
                        <div class="hero__search__phone">
                            <div class="hero__search__phone__icon">
                                <i class="fa fa-phone"></i>
                            </div>
                            <div class="hero__search__phone__text">
                                <h5>+90 555 111 22 33</h5>
                                <span>7/24 destek hattı</span>
                            </div>
                        </div>
                    </div>
                    <div class="hero__item set-bg" data-setbg="https://bogatyr.club/uploads/posts/2023-02/3351/1675513205_bogatyr-club-p-serii-gradient-foni-4.jpg">
                        <div class="hero__text">
                            <span>YENİ SEZON</span>
                            <h2>VELURIUM <br /></h2>
                            <p>Ücretsiz teslimat ve mağazadan teslim alma</p>
                            <a href="#" class="primary-btn">ALIŞVERİŞE BAŞLA</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Hero Section End -->
    <!-- Categories Section Begin -->
    @Html.Action("PartialCategory", "Default")
    <!-- Categories Section End -->
    <!-- Featured Section Begin -->
    @Html.Action("PartialFeature", "Default")
    <!-- Featured Section End -->
    <!-- Banner Begin -->
    @Html.Action("PartialBanner", "Default")
    <!-- Banner End -->
    <!-- Latest Product Section Begin -->
    @Html.Action("PartialProduct", "Default")
    <!-- Latest Product Section End -->
    <!-- Blog Section Begin -->
    @Html.Action("PartialBlog", "Default")
    <!-- Blog Section End -->
    <!-- Footer Section Begin -->
    @Html.Action("PartialFooter", "Default")
    <!-- Footer Section End -->
    <!-- Js Plugins -->
    <script src="~/ogani-master/ogani-master/js/jquery-3.3.1.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/bootstrap.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/jquery.nice-select.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/jquery-ui.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/jquery.slicknav.js"></script>
    <script src="~/ogani-master/ogani-master/js/mixitup.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/owl.carousel.min.js"></script>
    <script src="~/ogani-master/ogani-master/js/main.js"></script>



</body>
</html>
